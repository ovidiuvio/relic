# PostgreSQL Database Configuration
POSTGRES_USER=relic_user
POSTGRES_PASSWORD=relic_password
POSTGRES_DB=relic_db

# MinIO Object Storage Configuration
MINIO_ROOT_USER=minioadmin
MINIO_ROOT_PASSWORD=minioadmin
MINIO_BUCKET=relics

# Backend Configuration
# A comma-separated list of allowed origins for CORS.
# Example: "http://localhost,https://your-domain.com"
ALLOWED_ORIGINS="http://localhost"

# S3 Bucket Sync Service (Optional)
# To enable, set SYNC_ENABLED to "true" and fill in your S3 details.
S3_SYNC_ENABLED=false
S3_SYNC_ENDPOINT=s3.amazonaws.com
S3_SYNC_ACCESS_KEY=
S3_SYNC_SECRET_KEY=
S3_SYNC_BUCKET=relics-backup
S3_SYNC_REGION=us-east-1
S3_SYNC_INTERVAL=1800
S3_SYNC_TIMEZONE=UTC
